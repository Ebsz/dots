# overrides the default git aliases in zsh
alias gs='git status'
alias gap='git add -p'
alias gl='git log --pretty=oneline'

# configuration file aliases
alias cfg='/usr/bin/git --git-dir=$HOME/.cfg --work-tree=$HOME'
alias cfgs='cfg status'
alias cfga='cfg add'
alias cfgap='cfg add -p'
alias cfgl='cfg log --pretty=oneline'

alias free='free -h'
alias lsd="lsd --icon never"
alias up='ping 1.1.1.1'

alias r='ranger'

alias wb='west build --pristine'
alias wf='west flash'

alias sp='spotifyd'
alias ksp='killall spotifyd'

